{
  "$schema": "../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "excalidraw-app",
  "version": "0.1.0",
  "identifier": "com.imtotem.dev",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "yarn start",
    "beforeBuildCommand": "yarn build"
  },
  "app": {
    "windows": [
      {
        "title": "excalidraw-app",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
  }
],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "13.3"
    }
  }
}
